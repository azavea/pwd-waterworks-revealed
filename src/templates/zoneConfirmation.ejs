<div class="enterarea-container">
    <div class="enterarea-image" style="background-image: url('<%= mediaPath %>');">
    </div>
    <h3 class="text-center enterarea-message">
        You are in the <%= zone.title %> zone
    </h3>
    <div class="text-center">
        <button class="btn enterarea-button btn-secondary btn-lg btn-large dismiss">Dismiss</button>
        <button class="btn enterarea-button btn-primary btn-lg btn-large enter">Enter</button>
    </div>
</div>
